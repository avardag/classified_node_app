<h1>New Post</h1>

<form action="/posts" method="POST">
  <div>
    <input type="text" name="post[title]" placeholder="Title" >
  </div>
  <div>
    <input type="text" name="post[price]" placeholder="Price" >
  </div>
  <div>
    <textarea name="post[description]" id=""  placeholder="Description" ></textarea>
  </div>
  <div>
    <input type="text" name="post[location]" placeholder="Location" >
  </div>
  <div>
    <input type="submit">
  </div>
    
</form>

